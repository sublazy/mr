[$HOME/.config/vcsh/repo.d/ranger.git]
checkout = vcsh clone https://github.com/sublazy/rangerrc.git ranger
update   = vcsh ranger pull
push     = vcsh ranger remote set-url origin git@github.com:sublazy/rangerrc.git
           vcsh ranger remote set-url srht git@git.sr.ht:~nutfan/rangerrc
           vcsh ranger push srht
status   = vcsh ranger status
gc       = vcsh ranger gc
