[$HOME/.config/vcsh/repo.d/roxterm.git]
checkout = vcsh clone https://github.com/sublazy/roxtermrc.git roxterm
update   = vcsh roxterm pull
push     = vcsh roxterm remote set-url origin git@github.com:sublazy/roxtermrc.git
           vcsh roxterm push
status   = vcsh roxterm status
gc       = vcsh roxterm gc
