[$HOME/.config/vcsh/repo.d/zshrc.git]
checkout = vcsh clone https://github.com/sublazy/zshrc.git zshrc
update   = vcsh zshrc pull
push     = vcsh zshrc remote set-url origin git@github.com:sublazy/zshrc.git
           vcsh zshrc remote set-url srht git@git.sr.ht:~nutfan/zshrc
           vcsh zshrc push srht
status   = vcsh zshrc status
gc       = vcsh zshrc gc
srht     = vcsh zshrc remote add srht git@git.sr.ht:~nutfan/zshrc
