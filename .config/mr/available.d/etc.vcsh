[$HOME/.config/vcsh/repo.d/etc.git]
checkout = vcsh clone https://github.com/sublazy/etc.git etc
update   = vcsh etc pull
push     = vcsh etc remote set-url origin git@github.com:sublazy/etc.git
           vcsh etc remote set-url srht git@git.sr.ht:~nutfan/etc
           vcsh etc push srht
status   = vcsh etc status
gc       = vcsh etc gc
srht     = vcsh etc remote add srht git@git.sr.ht:~nutfan/etc
