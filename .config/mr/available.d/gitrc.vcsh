[$HOME/.config/vcsh/repo.d/gitrc.git]
checkout = vcsh clone https://github.com/sublazy/gitrc.git gitrc
update   = vcsh gitrc pull
push     = vcsh gitrc remote set-url origin git@github.com:sublazy/gitrc.git
           vcsh gitrc remote set-url srht git@git.sr.ht:~nutfan/gitrc
           vcsh gitrc push srht
status   = vcsh gitrc status
gc       = vcsh gitrc gc
